<script>
 

$(document).ready(function() {

       $.ajax({
        method: "GET",
        url:"/listradios",
        async: true,
        success: function(data) {
            listradio = data[0];

            console.log(listradio.radios[0].nom)

            let mytab = "<table id='tab' class='border0'><tbody>";
            for(i=0; i<listradio.radios.length; i++)
            {
                mytab += "<tr><td><img class='img-radio' src='"+listradio.radios[i].avatar+"'/></td><td class='text-center'>" +listradio.radios[i].nom+ "</td><td>"+listradio.radios[i].url+ "</td></tr>"
               //mytab += "<tr><td>"+ listradio.radios[i].avatar +"</td><td>" + listradio.radios[i].nom + "</td><td>" +listradio.radios[i].url+ "</td></tr>";
            }
            mytab += "</tbody></table>";
            $("#accueil").html(mytab);
        }
    })
}) 

 </script>